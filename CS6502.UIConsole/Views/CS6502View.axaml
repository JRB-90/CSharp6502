<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:shared="using:CS6502.UIConsole.Shared"
             mc:Ignorable="d" d:DesignWidth="620" d:DesignHeight="512"
             x:Class="CS6502.UIConsole.Views.CS6502View">
	<Grid>
		<shared:AspectRatioLayoutDecorator AspectRatio="{Binding Console.AspectRatio}">
			<ContentControl
				Content="{Binding Console}"/>
		</shared:AspectRatioLayoutDecorator>
	</Grid>
</UserControl>
